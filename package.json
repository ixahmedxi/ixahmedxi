{
  "name": "ixahmedxi",
  "version": "0.0.1",
  "private": true,
  "description": "Ahmed Elsakaan's personal portfolio website built with Astrojs",
  "license": "AGPL-3.0-or-later",
  "author": "Ahmed Elsakaan",
  "type": "module",
  "scripts": {
    "build": "astro check && astro build",
    "clean": "rimraf --glob .astro node_modules dist bun.lockb styled-system",
    "dev": "astro dev",
    "format": "bun format:write",
    "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
    "format:write": "prettier \"**/*\" --ignore-unknown --list-different --write",
    "lint": "eslint --ext .astro,.cjs,.mjs,.js,.jsx,.ts,.tsx . --max-warnings 0",
    "lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\"",
    "lint:spelling": "cspell \"**\"",
    "prepare": "husky install && panda codegen",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.1",
    "astro": "^4.1.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.3.4",
    "@astrojs/mdx": "^2.0.3",
    "@astrojs/sitemap": "^3.0.4",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@pandacss/astro": "^0.24.2",
    "@pandacss/dev": "^0.24.2",
    "@radix-ui/colors": "^3.0.0",
    "@types/eslint": "^8.56.1",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "cspell": "latest",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "npm:eslint-plugin-i@latest",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-regexp": "^2.1.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "markdownlint": "^0.32.1",
    "markdownlint-cli": "^0.38.0",
    "pandacss-preset-radix-colors": "^0.2.0",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.3",
    "prettier-plugin-curly": "^0.1.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-packagejson": "^2.4.8",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
